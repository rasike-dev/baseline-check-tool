{
  "Baseline Check TypeScript": {
    "prefix": "baseline-check-ts",
    "body": [
      "// @baseline-check: ${1:feature-name}",
      "// Status: ${2|baseline_like,risky,unknown|}",
      "// Browser Support: ${3:Chrome 42+, Firefox 39+, Safari 10.1+}",
      "// TypeScript: ${4:3.7+}",
      "// MDN: ${5:https://developer.mozilla.org/en-US/docs/Web/API/feature}"
    ],
    "description": "Add baseline compatibility comment for TypeScript"
  },
  "TypeScript Feature Detection": {
    "prefix": "ts-feature-detect",
    "body": [
      "interface ${1:FeatureName}Support {",
      "  supported: boolean;",
      "  version?: string;",
      "}",
      "",
      "function detect${1:FeatureName}(): ${1:FeatureName}Support {",
      "  if (typeof ${2:feature} !== 'undefined') {",
      "    return { supported: true, version: '${3:1.0}' };",
      "  }",
      "  return { supported: false };",
      "}",
      "",
      "const support = detect${1:FeatureName}();",
      "if (support.supported) {",
      "  ${4:// Use the feature}",
      "} else {",
      "  ${5:// Fallback implementation}",
      "}"
    ],
    "description": "TypeScript feature detection with types"
  },
  "Modern TypeScript Features": {
    "prefix": "modern-ts",
    "body": [
      "// @baseline-check: TypeScript modern features",
      "// Status: baseline_like",
      "// TypeScript: 4.0+",
      "",
      "interface ${1:Config} {",
      "  ${2:property}?: string;",
      "  ${3:optional}?: number;",
      "}",
      "",
      "function ${4:processConfig}(config: ${1:Config}) {",
      "  // Optional chaining",
      "  const value = config.${2:property}?.toUpperCase();",
      "  ",
      "  // Nullish coalescing",
      "  const defaultValue = config.${3:optional} ?? 42;",
      "  ",
      "  // Template literal types",
      "  type ${5:EventName} = \`${6:on}\${string}\`;",
      "  ",
      "  return { value, defaultValue };",
      "}"
    ],
    "description": "Modern TypeScript features with baseline checks"
  },
  "Generic Feature Detection": {
    "prefix": "generic-detect",
    "body": [
      "type FeatureSupport<T> = {",
      "  feature: T;",
      "  supported: boolean;",
      "  fallback?: () => void;",
      "};",
      "",
      "function createFeatureDetector<T>(",
      "  feature: T,",
      "  check: () => boolean,",
      "  fallback?: () => void",
      "): FeatureSupport<T> {",
      "  return {",
      "    feature,",
      "    supported: check(),",
      "    fallback",
      "  };",
      "}",
      "",
      "const ${1:featureName}Support = createFeatureDetector(",
      "  '${2:feature-name}',",
      "  () => typeof ${3:feature} !== 'undefined',",
      "  () => { ${4:// Fallback implementation} }",
      ");"
    ],
    "description": "Generic feature detection with TypeScript"
  },
  "Web API with Types": {
    "prefix": "web-api-ts",
    "body": [
      "// @baseline-check: ${1:Web API}",
      "// Status: ${2|baseline_like,risky,unknown|}",
      "// Browser Support: ${3:Chrome 42+, Firefox 39+, Safari 10.1+}",
      "",
      "interface ${4:API}Options {",
      "  ${5:option1}?: boolean;",
      "  ${6:option2}?: string;",
      "}",
      "",
      "interface ${4:API}Result {",
      "  success: boolean;",
      "  data?: any;",
      "  error?: string;",
      "}",
      "",
      "function use${4:API}(options: ${4:API}Options = {}): Promise<${4:API}Result> {",
      "  return new Promise((resolve) => {",
      "    if (typeof ${1:api} !== 'undefined') {",
      "      // Use modern API",
      "      ${1:api}(${7:options})",
      "        .then((result: any) => resolve({ success: true, data: result }))",
      "        .catch((error: Error) => resolve({ success: false, error: error.message }));",
      "    } else {",
      "      // Fallback implementation",
      "      resolve({ success: false, error: 'API not supported' });",
      "    }",
      "  });",
      "}"
    ],
    "description": "Web API with TypeScript types and fallback"
  }
}
